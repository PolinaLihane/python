def f(x):
    x**2
g = f 
print(f(1))
print(g(1))

def f(x):
    x**2

print(f(2))

def calc1(x):
    return x+10

def calc2(x):
    return x*10

def math(op, x):
    print(op(x))

math(calc2, 10)

def sum(x, y):
    return x+y
# получим такую лямбду:
sum = lambda x, y: x+y

def mylt(x,y):
    return x*y

def calc(op, a, b):  #op это мульт тут
    print(op(a, b))

calc(lambda x, y: x+y, 4 , 5)